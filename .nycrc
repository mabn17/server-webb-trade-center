{
    "extends": "@istanbuljs/nyc-config-typescript",
    "description": "",
    "reporter": [
      "lcov",
      "clover",
      "html",
      "text"
    ],
    "require": [
        "ts-node/register"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "coverage/",
      "src/lib",
      "src/setup.ts"
    ],
    "all": true,
    "report-dir": ".coverage",
    "temp-directory": "./.nyc_output"
}
